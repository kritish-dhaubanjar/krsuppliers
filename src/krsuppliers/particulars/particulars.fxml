<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefWidth="960.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="krsuppliers.particulars.ParticularsController">
    <children>
        <TableView fx:id="table" prefHeight="640.0" prefWidth="660.0">
            <columns>
                <TableColumn fx:id="_particular_id" prefWidth="100.0" text="PID" />
                <TableColumn fx:id="_particular" prefWidth="240.0" text="Particular" />
            </columns>
        </TableView>
        <Pane layoutX="660.0" prefHeight="640.0" prefWidth="300.0" style="-fx-background-color: #f3f3f3f3;">
            <children>
                <Label layoutX="23.0" layoutY="30.0" text="Particulars">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="100.0" text="Name">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
            <TextField fx:id="name" layoutX="99.0" layoutY="96.0" prefHeight="25.0" prefWidth="175.0" />
            <JFXButton fx:id="save" buttonType="RAISED" layoutX="215.0" layoutY="138.0" style="-fx-background-color: #00BC58;" text="SAVE" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="cancel" buttonType="RAISED" layoutX="119.0" layoutY="138.0" style="-fx-background-color: #F55E73;" text="CANCEL" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </JFXButton>
            </children></Pane>
    </children>

</AnchorPane>
